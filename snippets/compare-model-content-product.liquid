                  <span>swipe left to see more</span>
                  {% render 'icon' with 'icon-swipe-text' %}
                </div>
              </div>
              {% endcomment %}
            </div>
            <div class="d-flex rows_info col-{{ len }}">
              {% for i in (1..3) %}
                {% assign p_handle = 'product' | append: i %}
                {% assign icon_handle = 'icon' | append: i %}
                {% assign price_handle = 'price' | append: i %}
