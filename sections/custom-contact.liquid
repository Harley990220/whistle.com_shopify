{% comment %} ATTENTION!!! This is compiled version of custom-contact.liquid and it is overriding automatically. You should use theme development tool and sources\sections\custom-contact.liquid file to code and save your changes. {% endcomment %}
{% if section.settings.visible %}

  {% render 'ea-section-styles', section: section %}

  {% render 'custom-contact-content', section: section %}

{% endif %}

{% schema %}
  {
  "name": "Contact",
  "class": "contact-info",
  "tag": "section",
  "settings": [
    {
      "type": "checkbox",
      "id": "visible",
      "label": "Show block",
      "default": true
    },
    {
      "type": "header",
      "content": "Spacing"
    },
    {
      "type": "range",
      "id": "ptop",
      "label": "Top spacing desktop",
      "min": 0,
      "unit": "px",
      "max": 500,
      "step": 5,
      "default": 45
    },
    {
      "type": "range",
      "id": "pbottom",
      "label": "Bottom spacing desktop",
      "min": 0,
      "unit": "px",
      "max": 500,
      "step": 5,
      "default": 45
    },
    {
      "type": "range",
      "id": "ptop_mob",
      "label": "Top spacing mobile",
      "min": 0,
      "unit": "px",
      "max": 500,
      "step": 5,
      "default": 45
    },
    {
      "type": "range",
      "id": "pbottom_mob",
      "label": "Bottom spacing mobile",
      "min": 0,
      "unit": "px",
      "max": 500,
      "step": 5,
      "default": 45
    },
    {
      "type": "header",
      "content": "Background"
    },
    {
      "type": "color",
      "id": "sectionbg",
      "label": "Background",
      "default": "#fff"
    },
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    },
    {
      "type": "richtext",
      "id": "subtitle",
      "label": "Sub title"
    },
    {
      "type": "header",
      "content": "Phone"
    },
    {
      "type": "text",
      "id": "phone_title",
      "label": "Title",
      "default": "CALL US"
    },
    {
      "type": "text",
      "id": "phone_num",
      "label": "Phone number"
    },
    {
      "type": "header",
      "content": "Chat Us"
    },
    {
      "type": "text",
      "id": "chat_title",
      "label": "Title",
      "default": "CHAT WITH US"
    }
  ],
  "blocks": [
    {
      "type": "block",
      "name": "Block",
      "limit": 5,
      "settings": [
        {
          "type": "html",
          "id": "icon",
          "label": "Icon"
        },
        {
          "type": "text",
          "id": "title",
          "label": "Title"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Contact Info",
      "category": "Contact"
    }
  ]
}
{% endschema %}